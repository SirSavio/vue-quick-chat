{
    "name": "vue-quick-chat",
    "version": "1.2.8",
    "private": false,
    "main": "./dist/vue-quick-chat.common.js",
    "files": [
        "dist/*",
        "src/*",
        "public/*",
        "*.json",
        "*.js"
    ],
    "author": "Matheus Santos <matheus.r.d.s10@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "vue",
        "vue-js-chat",
        "chat",
        "vuejs",
        "simple-chat"
    ],
    "repository": "https://github.com/MatheusrdSantos/vue-quick-chat",
    "scripts": {
        "serve": "vue-cli-service serve",
